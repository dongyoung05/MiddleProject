package kr.or.ddit.vo;

public class SalonVo {
	
	private String salon_no;
	private String salon_name;
	private int salon_price;
	private String salon_desc;
	private String salon_img;
	private String salon_weight;
	
	
	public String getSalon_no() {
		return salon_no;
	}
	public void setSalon_no(String salon_no) {
		this.salon_no = salon_no;
	}
	public String getSalon_name() {
		return salon_name;
	}
	public void setSalon_name(String salon_name) {
		this.salon_name = salon_name;
	}
	public int getSalon_price() {
		return salon_price;
	}
	public void setSalon_price(int salon_price) {
		this.salon_price = salon_price;
	}
	public String getSalon_desc() {
		return salon_desc;
	}
	public void setSalon_desc(String salon_desc) {
		this.salon_desc = salon_desc;
	}
	public String getSalon_img() {
		return salon_img;
	}
	public void setSalon_img(String salon_img) {
		this.salon_img = salon_img;
	}
	public String getSalon_weight() {
		return salon_weight;
	}
	public void setSalon_weight(String salon_weight) {
		this.salon_weight = salon_weight;
	}
	
}
